PS1="%n [%1~] %# "
echo Architecture: `arch`

export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

export PATH=/opt/homebrew/bin:$PATH

SCRIPT_DIR="${0:A:h}"

source $SCRIPT_DIR/basic/aliases
source $SCRIPT_DIR/basic/functions
source $SCRIPT_DIR/basic/help
source $SCRIPT_DIR/git/aliases
source $SCRIPT_DIR/git/functions
source $SCRIPT_DIR/git/help
source $SCRIPT_DIR/vim/help
